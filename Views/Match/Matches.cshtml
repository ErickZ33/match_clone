<div id="dash-nav">
<div class="left-bar"><a href="dashboard" class="maroon brand-logo">Sadness.com</a></div>
<div class="middle-bar">
<div class="center-help">
<div class="hover-square"><a class="no-underline" href="/profile">Profile</a></div>
<div class="hover-square"><a class="no-underline" href="/lovers">Matches</a></div>
<div class="hover-square"><a class="no-underline" href="#">Messages</a></div>
<div class="hover-square"><a class="no-underline" href="/likes">Likes</a></div>
</div>
</div>
<div class="right-bar">
<a class='dropdown-button' href='' data-activates='dropdownAccount'><i class="material-icons sad maroon">sentiment_dissatisfied</i></a>
<a class='dropdown-button' href='#' data-activates='dropdownSettings'><i class="material-icons sad maroon">settings</i></a>
</div>
</div>

<div class="body-container">
  <div class="body-stuff2">
    @{
        foreach(var match in @ViewBag.myLovers){
            string percentStr = @match.percentage + "% Match";
            <div class="lover-cube">
                <img id="profP" src="@Url.Content(match.lover.profile_picture)"/>
                <p><a class="link-text" href="/lovers/@match.lover.UserId">@match.lover.name</a>, @match.lover.age years old</p>
                <p>@percentStr</p>
                <a href="/like/@match.lover.UserId" class="link-text"><button>Like</button></a><a class="link-text" href="/conversation/@match.lover.UserId"><button>Message</button></a>
            </div>
        }
    }
  </div>
</div>
